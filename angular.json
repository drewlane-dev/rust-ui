    "discogs-notifier-web": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/discogs-notifier-web",
      "sourceRoot": "apps/discogs-notifier-web/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/discogs-notifier-web",
            "index": "apps/discogs-notifier-web/src/index.html",
            "main": "apps/discogs-notifier-web/src/main.ts",
            "polyfills": "apps/discogs-notifier-web/src/polyfills.ts",
            "tsConfig": "apps/discogs-notifier-web/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/discogs-notifier-web/src/favicon.ico",
              "apps/discogs-notifier-web/src/assets",
              "apps/discogs-notifier-web/src/config.json"
            ],
            "styles": [
              "apps/discogs-notifier-web/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/discogs-notifier-web/src/environments/environment.ts",
                  "with": "apps/discogs-notifier-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "discogs-notifier-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "discogs-notifier-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "discogs-notifier-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/discogs-notifier-web/src/test.ts",
            "polyfills": "apps/discogs-notifier-web/src/polyfills.ts",
            "tsConfig": "apps/discogs-notifier-web/tsconfig.spec.json",
            "karmaConfig": "apps/discogs-notifier-web/karma.conf.js",
            "assets": [
              "apps/discogs-notifier-web/src/favicon.ico",
              "apps/discogs-notifier-web/src/assets"
            ],
            "styles": [
              "apps/discogs-notifier-web/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/discogs-notifier-web/tsconfig.app.json",
              "apps/discogs-notifier-web/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "discogs-notifier-web-e2e": {
      "projectType": "application",
      "root": "apps/discogs-notifier-web-e2e",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/discogs-notifier-web-e2e/protractor.conf.js",
            "devServerTarget": "discogs-notifier-web:serve"
