  "defaultProject": "discogs-notifier-web",
  "cli": {
    "defaultCollection": "@nxtend/ionic-angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "karma",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "karma"
    }
